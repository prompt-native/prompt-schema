{
  "$schema": "../schema/completion-schema-0.2.json",
  "version": "0.2",
  "type": "completion",
  "vendor": "google",
  "model": {
    "name": "text-bison",
    "version": "001"
  },
  "prompt": {
    "context": "You are going to show the structure of a word, use the format in examples.",
    "examples": [
      {
        "field": "input",
        "values": ["Hello"]
      },
      {
        "field": "output",
        "values": ["H-E-L-L-O, hello"]
      }
    ],
    "test": ["Foobar"]
  },
  "parameters": [
    {
      "name": "temperature",
      "value": 0.1
    }
  ]
}
